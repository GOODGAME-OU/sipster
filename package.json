{
    "name": "sipster",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "settings": {
        "name": "Sipster"
    },
    "scripts": {
        "start": "tsc -b && vite",
        "build": "tsc -b && vite build",
        "preview": "pnpm run build & vite preview",
        "lint": "prettier  src --write",
        "assets": "pwa-asset-generator public/assets/logo.png public/assets/icons --portrait-only --dark-mode -b \"linear-gradient(to right top, #242424 0%, #000000 100%)\" --path-override assets/icons --manifest public/assets/manifest.json"
    },
    "dependencies": {
        "firebase": "^11.0.1",
        "localstorage-slim": "^2.7.1",
        "pwa-install-handler": "^2.6.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.4.0",
        "react-router-dom": "^6.27.0",
        "react-spinners": "^0.15.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "@vite-pwa/assets-generator": "^0.2.6",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^9.15.0",
        "eslint-plugin-react": "^7.37.2",
        "globals": "^15.11.0",
        "pwa-asset-generator": "^6.4.0",
        "typescript": "~5.6.2",
        "vite": "^5.4.9",
        "vite-plugin-conditional-compiler": "^0.3.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-pwa": "^0.21.1"
    }
}
